{
  "name": "@drstrangediv/static-wc",
  "version": "1.0.0",
  "description": "Render the shadowRoot of custom elements to static HTML",
  "license": "MIT",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "format": "run-s format:*",
    "format:eslint": "eslint --ext .js,.html,.md . --fix",
    "format:prettier": "prettier \"**/*.{js,html,md}\" \"**/package.json\" --write",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ext .js,.html,.md .",
    "lint:prettier": "prettier \"**/*.{js,html,md}\" \"**/package.json\" --check",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@node-loader/core": "^1.0.3",
    "@node-loader/import-maps": "^1.0.3",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "es-module-lexer": "^0.3.26",
    "globals": "^12.4.0",
    "is-potential-custom-element-name": "^1.0.0",
    "jsdom": "^16.4.0",
    "lit-element": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qsa-observer": "^1.1.6",
    "rehype": "^11.0.0",
    "shelljs": "^0.8.4",
    "strip-comments": "^2.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-jsdoc": "^31.6.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "prettier-plugin-package": "^1.3.0"
  },
  "contributors": [
    "John Loy <loy.john@gmail.com> (https://frameworkless-web.dev)"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,html,md}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  }
}
